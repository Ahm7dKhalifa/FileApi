{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Files": {
    "MaxFileLength": 1024,
    "AllowedFiles": [
      {
        "format": "xml",
        "contentType": "text/xml",
        "canBeExportedTo": [ "json", "yml" ]
      },
      {
        "format": "yml",
        "contentType": "text/yml",
        "canBeExportedTo": [ "json", "xml" ]
      },
      {
        "format": "json",
        "contentType": "application/json",
        "canBeExportedTo": [ "xml", "yml" ]
      },
      {
        "format": "txt",
        "contentType": "text/plain"
      },
      {
        "format": "csv",
        "contentType": "text/csv"
      }
    ]
  }
  }
